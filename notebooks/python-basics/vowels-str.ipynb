   "source": [
    "def count_vowels(s):\n",
    "    vowels = \"aeiouAEIOU\"\n",
    "    count = 0\n",
    "    i = 0\n",
    "    while i < len(s):\n",
    "        if s[i] in vowels:\n",
    "            count += 1\n",
    "        i += 1\n",
    "    print(\"Number of vowels:\", count)\n",
    "\n",
    "# Example usage\n",
    "count_vowels(\"hello world\")\n"
   ]
